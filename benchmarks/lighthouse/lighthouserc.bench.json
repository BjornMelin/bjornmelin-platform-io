{
  "ci": {
    "collect": {
      "staticDistDir": "../../../out",
      "numberOfRuns": 3,
      "url": ["/index.html", "/about/index.html", "/projects/index.html", "/contact/index.html"],
      "settings": {
        "preset": "desktop",
        "chromeFlags": "--user-data-dir=./.chrome-profile --no-sandbox --disable-gpu --disable-dev-shm-usage",
        "throttling": {
          "cpuSlowdownMultiplier": 1
        },
        "maxWaitForLoad": 45000
      }
    }
  }
}
